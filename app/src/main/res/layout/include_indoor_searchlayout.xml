<?xml version="1.0" encoding="utf-8"?>

<layout
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data class = "IncludeSearchBinding">

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/MAIN"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/include_indoor_search_rlyt_back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="22dp"
                android:layout_height="18dp"
                android:layout_marginLeft="18dp"
                android:background="@drawable/ic_indoor_back"/>

        </RelativeLayout>


        <LinearLayout
            android:id="@+id/include_indoor_search_et_searchlayout"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="17dp"
            android:orientation="horizontal"
            android:background="@drawable/button_grey_radius_stroke"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="11dp"
                android:background="@drawable/ic_indoor_search"/>

            <TextView
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="支持在室内搜索"
                android:padding="5dp"/>
        </LinearLayout>



    </LinearLayout>

</layout>
