<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="PathPlanActivityBinding">

    </data>


    <android.support.constraint.ConstraintLayout
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/activity_path_plan_ed_start"
            android:text="我的位置"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:padding="5dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/activity_path_plan_btn_change"/>

        <EditText
            android:id="@+id/activity_path_plan_ed_end"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:padding="5dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/activity_path_plan_btn_change"
            app:layout_constraintTop_toBottomOf="@+id/activity_path_plan_ed_start"/>

        <Button
            android:id="@+id/activity_path_plan_btn_change"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="换"
            app:layout_constraintBottom_toBottomOf="@+id/activity_path_plan_ed_end"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/activity_path_plan_ed_start"
            />


        <Button
            android:id="@+id/activity_path_plan_btn_search"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="搜索"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/activity_path_plan_ed_end"/>


    </android.support.constraint.ConstraintLayout>

</layout>